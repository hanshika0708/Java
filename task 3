package SwitchCase;
ublic class SwitchCase {

   public static void main(String args[]){
      int choice;
      system.out.println("pick one: 1. Hi\t2. Hello\t3. Hey\t");
      scanner s = new scanner(System.in);
      choice = s.nextInt();
      switch(choice)
      {
	 case 1:
	   System.out.println("You choiced Hi");
     break;
	 case 2:
	   System.out.println("You choiced Hello");
     break;
	 case 3:
	   System.out.println("You choiced Hey");
     break;
	 default:
	   System.out.println("Wrong Choice ");
      }
   }
}
